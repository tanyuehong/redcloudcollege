<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.redskt.classroom.mapper.RedInterviewPositionMapper">

    <select id="getInterviewPositionTagList" resultType="com.redskt.classroom.entity.RedCategoryTag">
        SELECT tag.*
        FROM red_category_tag tag
        LEFT OUTER JOIN red_interview_position_tags tTags ON tTags.tid = tag.id
        where tTags.pid = #{pId}
    </select>

</mapper>
